   "source": [
    "# 方式1:使用print直接输出类型信息\n",
    "\n",
    "print(type(\"黑马程序员\"))\n",
    "print(type(666))\n",
    "print(type(13.14))\n"
   ]
  },
  {
   "cell_type": "raw",
   "id": "6ac79e3c-8b05-4382-8e96-d181264bc6d9",
   "metadata": {
    "vscode": {
     "languageId": "raw"
    }
   },
   "source": [
    "# 方式2:使用变量存储type()语句的结果\n",
    "\n",
    "string_type = type(\"黑马程序员\")\n",
    "int_type = type(666)\n",
    "float_type = type(13.14)\n",
    "print(string_type)\n",
    "print(int_type)\n",
    "print(float_type)"
   ]
  },
  {
   "cell_type": "code",
